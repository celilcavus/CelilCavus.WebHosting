@model List<AboutUs>


    <div class="card">
        <div class="card-body">
            <h5 class="card-title">About List</h5>
            <div class="table-responsive">
                <table id="zero_config" class="table table-striped table-bordered">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Header</th>
                            <th>Image</th>
                            <th>Description</th>
                            <th>Update</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.Id</td>
                                <td>@item.Header</td>
                                <td>@item.Image</td>
                                <td>@item.Description</td>
                                <td><a href="/AboutUs/Update/@item.Id" class="btn btn-outline-warning">Update</a></td>
                                <td><a href="/AboutUs/Delete/@item.Id" class="btn btn-outline-danger">Delete</a></td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>

        </div>
    </div>